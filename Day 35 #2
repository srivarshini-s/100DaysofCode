class Solution {

    void printTriangle(int n) {
        // code here
        char a='A';
        for(int i=0;i<n;i++){
            for(char j='A';j<='A'+i;j++){
                System.out.print(a);
            }
             a+=1;
            System.out.println();
        }
    }
}
